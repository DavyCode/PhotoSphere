<% include partials/head%>
<% include partials/header%>

    <div class="container">
        <div class="row">
            <h1 style="text-align: center;">Upload Your Latest Photo</h1>
            <div style="width:30%; margin:25px auto;">
                <form action="/home" method="Post">
                    <div class="form-group">
                        <input class="form-control" type="text" name="image" placeholder="Image url">
                    </div>
                    <div class="form-group">
                        <input class="form-control" type="text" name="caption" placeholder="caption">
                    </div>
                    <div class="form-group">
                        <button class="btn btn-lg btn-primary btn-block" btn>Submit!</button>
                    </div>
                </form>
                <a href="/home">Back to Photo Gallery</a>
            </div>
        </div>
    </div>

<% include partials/foot%>
